{
  "stack": "dotnet",
  "memory": 1024,
  "timeout": 300,
  "cpu": {
    "type": "shared",
    "value": 80
  },
  "minReplicas": 8,
  "maxReplicas": 80,
  "runtimeArgs": [],
  "routes": {
    "reviewApiAction": {
      "path": "/reviews-and-ratings/api/:requestedAction",
      "public": false
    },
    "reviewApiActionId": {
      "path": "/reviews-and-ratings/api/:requestedAction/:id",
      "public": false
    },
    "verifySchema": {
      "path": "/reviews-and-ratings/verify-schema",
      "public": true
    },
    "migrateData": {
      "path": "/reviews-and-ratings/migrate-data",
      "public": false
    },
    "addSearchDate": {
      "path": "/reviews-and-ratings/add-search-date",
      "public": true
    }
  },
  "events": {
    "onAppsLinked": {
      "sender": "apps",
      "keys": ["linked"]
    }
  }
}
